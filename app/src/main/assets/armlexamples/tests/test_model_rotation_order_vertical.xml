
<arml xmlns="http://www.opengis.net/arml/2.0" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink">
	<ARElements>
		
		<Model id="axisModel">
			<href xlink:href="models/misc/XYZ.glb" />
			<Scale>
				<x>0.005</x>
				<y>0.005</y>
				<z>0.005</z>
			</Scale>
			<Orientation>
				<roll>90</roll>
			</Orientation>
		</Model>

		<Feature id="testFeature">
			<anchors>

				<Trackable id="planeAnchor">
					<config>
						<tracker xlink:href="#genericVPlaneTracker" />
						<src>look for a plane, idk</src>
					</config>
					<assets>
						<assetRef xlink:href="axisModel" />
						<Model>
							<href xlink:href="models/misc/Teapot.glb" />
							<Scale>
								<x>0.05</x>
								<y>0.05</y>
								<z>0.05</z>
							</Scale>
							<Orientation>
								<tilt>90</tilt>
								<roll>90</roll>
								<heading>90</heading>
							</Orientation>
						</Model>
					</assets>
				</Trackable>

			</anchors>
		</Feature>

	</ARElements>
</arml>
